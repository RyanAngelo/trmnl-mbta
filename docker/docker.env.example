# TRMNL MBTA Docker Environment Configuration
# Copy this file to docker.env and fill in your actual values

# MBTA API Configuration
# Get your API key from https://api-v3.mbta.com/
MBTA_API_KEY=your_mbta_api_key_here

# TRMNL Configuration
# Get this from your TRMNL dashboard
TRMNL_WEBHOOK_URL=https://api.trmnl.com/your_webhook_url_here



# Debug Settings
# Set to true for development, false for production
DEBUG_MODE=false

# Debug output file (leave empty to output to console)
DEBUG_OUTPUT_FILE=

# Docker-specific settings
COMPOSE_PROJECT_NAME=trmnl-mbta
